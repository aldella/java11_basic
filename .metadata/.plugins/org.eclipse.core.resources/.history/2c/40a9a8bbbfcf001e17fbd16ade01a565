package org.kh.java;

public class Constant1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 상수(Constant)와 리터럴(Literal)
		final int MAX_POINT = 100;
		final int MIN_POINT = 0;
		final double PI = 3.141592;
		final String MY_NAME = "최태영";
		//상수는 값을 변화시킬 수 없음. 변수는 가능
		//변수나 상수에 대입되는 값을 리터럴(Literal)이라고 함
		System.out.printf("%d %d %f %s %n", MAX_POINT, MIN_POINT, PI, MY_NAME);
		int a = 10;
		float b = 2.0f;
		System.out.println( (int)(a+b) );
		
		
	}

}
