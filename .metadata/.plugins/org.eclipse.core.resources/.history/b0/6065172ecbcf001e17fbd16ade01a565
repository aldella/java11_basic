package org.kh.java;

public class Operation5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		관계(비교)연산자 : ==, !=, >(초과), >=(이상), <(미만), <=(이하) : 결과는 true/false
		int a =25;
		int b = 28;
		int c = 27;
		System.out.println((a==b));
		System.out.println((a!=b));
		System.out.println((a>b));
		System.out.println((a<b));
		
	}

}
