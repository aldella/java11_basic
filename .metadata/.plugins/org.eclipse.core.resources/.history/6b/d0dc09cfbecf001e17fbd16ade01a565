package org.kh.java;

public class Constant1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 상수(Constant)와 리터럴(Literal)
		final int MAX_POINT = 100;
		final int MIN_POINT = 0;
		final double PI = 3.141592;
		final String MY_NAME = "최태영";
		//상수는 값을 변화시킬 수 없음. 변수는 가능
		//변수나 상수에 대입되는 값을 리터럴(Literal)이라고 함
		System.out.printf("%d %d %f %s", MAX_POINT, MIN_POINT, PI, MY_NAME);
		float a = 1.1f;
		double c = 2.2;
		int b = (int) c;
		System.out.println(a+b);
		
	}

}
