package org.kh.java;

public class Operation2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		산술연산(Arithmetic) <=> 논리연산(Logical)
//		+ - * / %    : 이 5가지가 끝
		int su1 = 98;
		int su2 = 18;
		System.out.println("su1 + su2 = "+(su1+su2));
		System.out.println("su1 - su2 = "+(su1-su2));
		System.out.println("su1 * su2 = "+(su1*su2));
		System.out.println("su1 / su2 = "+(su1/su2)); //나누기
		System.out.println("su1 % su2 = "+(su1%su2)); //나누기 한 나머지
		
		float a1 = 98;
		float a2 = 3.7;
		System.out.println(a1/a2);
	}

}
